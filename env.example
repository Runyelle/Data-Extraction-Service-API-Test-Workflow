# API Configuration
API_BASE_URL=http://localhost:8000 (or use a remote api url)
API_TIMEOUT=30

# Database Configuration (for seeded data tests)
# If you have direct database access for seeding test data
DB_HOST=localhost
DB_PORT=5432
DB_NAME=test_db
DB_USER=test_user
DB_PASSWORD=test_password

# Service Provided API Token (for real extraction tests)
# This should be a valid token for the third-party service
SERVICE_PROVIDED_API_TOKEN=your_token_here (real token in .env)

# Test Configuration
POLL_INTERVAL_SECONDS=5
MAX_POLL_ATTEMPTS=120
TEST_DATA_CLEANUP=true

